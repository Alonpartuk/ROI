# Environment Variables Template for HubSpot to BigQuery ETL
# Copy this file to .env.yaml and fill in your values
# DO NOT commit .env.yaml to version control!

# Google Cloud Project ID
PROJECT_ID: "your-gcp-project-id"

# BigQuery Dataset ID (will be created if doesn't exist)
DATASET_ID: "hubspot_data"

# BigQuery Table ID (will be created if doesn't exist)
TABLE_ID: "deals_snapshots"

# HubSpot Private App Access Token
# Get this from: HubSpot > Settings > Integrations > Private Apps
# Required scopes: crm.objects.deals.read
HUBSPOT_ACCESS_TOKEN: "pat-na1-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
